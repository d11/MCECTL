template <class Input, class Output> 
int 
pair<Input, Output>::operator<(const pair<Input, Output>& p) const 
{ 
	if (left < p.left) 
		return 1; 
	if (left > p.left) 
		return 0; 
	if (right < p.right) 
		return 1; 
	return 0; 
} 
